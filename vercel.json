{
    "version": 2,
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/v0/faqs(.*)",
        "dest": "src/server.ts"
      },
      {
        "src": "/api/v0/businesses(.*)",
        "dest": "src/server.ts"
      },
      {
        "src": "/api/v0/ai(.*)",
        "dest": "src/server.ts"
      },
      {
        "src": "/api/v0/wa(.*)",
        "dest": "src/server.ts"
      }
    ]
  }
  